{
  "name": "zova-dev",
  "description": "A vue3 framework with ioc",
  "author": "zhennann <zhen.nann@icloud.com>",
  "type": "module",
  "scripts": {
    "zova": "node ../zova-cli/cli/dist/bin/zova.js",
    "dev": "vite",
    "build": "vite build && tsc --noEmit --project tsconfig.vue-tsc.json",
    "preview": "vite preview",
    "tsc": "vue-tsc --noEmit --project tsconfig.vue-tsc.json",
    "lint": "eslint . --fix --ignore-path .gitignore"
  },
  "keywords": [
    "nodejs",
    "full-stack",
    "workflow-engine",
    "vue3",
    "ioc",
    "cabloy"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabloy/zova.git"
  },
  "homepage": "https://github.com/cabloy/zova#readme",
  "bugs": {
    "url": "https://github.com/cabloy/zova/issues"
  },
  "dependencies": {
    "@cabloy/front": "workspace:^",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@cabloy/app-vite": "workspace:^",
    "@cabloy/lint": "^4.0.10",
    "@types/node": "^20.12.13",
    "@vitejs/plugin-vue": "^5.0.5",
    "sass": "^1.77.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vue-tsc": "^2.0.19"
  },
  "license": "MIT"
}
