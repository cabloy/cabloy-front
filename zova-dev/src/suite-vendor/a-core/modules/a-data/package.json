{
  "name": "zova-module-a-data",
  "version": "5.0.0",
  "title": "a-data",
  "zovaModule": {
    "capabilities": {
      "monkey": true
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./*": "./*"
  },
  "description": "core",
  "keywords": [
    "Zova Module"
  ],
  "author": "zhennann",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "tsc -b --clean",
    "tsc:publish": "npm run clean && tsc -b",
    "prepublishOnly": "npm run tsc:publish"
  },
  "dependencies": {
    "@types/js-cookie": "^3.0.6",
    "@tanstack/vue-query": "^5.45.0",
    "@tanstack/query-persist-client-core": "^5.45.0",
    "js-cookie": "^3.0.5",
    "localforage": "^1.10.0"
  }
}
